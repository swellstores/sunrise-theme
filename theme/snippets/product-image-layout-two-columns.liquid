<div class="max-h-766">
  <div class="grid grid-cols-2 gap-16 overflow-y-auto w-full max-h-full scrollbar-hidden">
    {% for url in media_urls %}
      <div class="aspect-square w-full">
        <img
          src="{{ url }}"
          alt="{{ media_alts[forloop.index0] }}"
          class="w-full h-full object-cover rounded"
        >
      </div>
    {% endfor %}
  </div>
</div>